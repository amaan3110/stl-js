{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES2019", "DOM"],
    "module": "ESNext",
    "outDir": "dist",
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["index.ts", "structures/**/*"]
}
